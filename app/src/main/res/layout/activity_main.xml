<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text=""
        android:textSize="50dp"
        android:textColor="@color/colorWhite"
        android:layout_centerInParent="true"
        android:id="@+id/b22"/>
    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text=""
        android:textSize="50dp"
        android:textColor="@color/colorWhite"
        android:layout_above="@id/b22"
        android:layout_centerHorizontal="true"
        android:id="@+id/b12"/>
    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text=""
        android:textSize="50dp"
        android:textColor="@color/colorWhite"

        android:layout_above="@id/b22"
        android:layout_toLeftOf="@id/b12"
        android:id="@+id/b11"/>
    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text=""
        android:textSize="50dp"
        android:textColor="@color/colorWhite"
        android:layout_above="@id/b22"
        android:layout_toRightOf="@id/b12"
        android:id="@+id/b13"/>
    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text=""
        android:textSize="50dp"
        android:textColor="@color/colorWhite"
        android:layout_below="@id/b12"
        android:layout_toLeftOf="@id/b22"
        android:id="@+id/b21"/>
    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text=""
        android:textSize="50dp"
        android:textColor="@color/colorWhite"
        android:layout_below="@id/b12"
        android:layout_toRightOf="@id/b22"
        android:id="@+id/b23"/>
    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text=""
        android:textSize="50dp"
        android:textColor="@color/colorWhite"
        android:layout_below="@id/b22"
        android:layout_centerHorizontal="true"
        android:id="@+id/b32"/>
    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text=""
        android:textSize="50dp"
        android:textColor="@color/colorWhite"
        android:layout_below="@id/b22"
        android:layout_toLeftOf="@id/b32"
        android:id="@+id/b31"/>
    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text=""
        android:textSize="50dp"
        android:textColor="@color/colorWhite"
        android:layout_below="@id/b22"
        android:layout_toRightOf="@id/b32"
        android:id="@+id/b33"/>

    <Button
        android:id="@+id/newGame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NEW GAME"
        android:backgroundTint="@color/colorPrimary"
        android:textColor="#ffffff"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:visibility="gone"/>







</RelativeLayout>